{
  "multipart": [
    {
      "when": {
        "running": "false",
        "facing": "north"
      },
      "apply": {
        "model": "space:block/coal_generator"
      }
    },
    {
      "when": {
        "running": "false",
        "facing": "south"
      },
      "apply": {
        "model": "space:block/coal_generator",
        "y": 180
      }
    },
    {
      "when": {
        "running": "false",
        "facing": "west"
      },
      "apply": {
        "model": "space:block/coal_generator",
        "y": 270
      }
    },
    {
      "when": {
        "running": "false",
        "facing": "east"
      },
      "apply": {
        "model": "space:block/coal_generator",
        "y": 90
      }
    },
    {
      "when": {
        "running": "true",
        "facing": "north"
      },
      "apply": {
        "model": "space:block/coal_generator_running"
      }
    },
    {
      "when": {
        "running": "true",
        "facing": "south"
      },
      "apply": {
        "model": "space:block/coal_generator_running",
        "y": 180
      }
    },
    {
      "when": {
        "running": "true",
        "facing": "west"
      },
      "apply": {
        "model": "space:block/coal_generator_running",
        "y": 270
      }
    },
    {
      "when": {
        "running": "true",
        "facing": "east"
      },
      "apply": {
        "model": "space:block/coal_generator_running",
        "y": 90
      }
    }
  ]
}