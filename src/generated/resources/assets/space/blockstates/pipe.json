{
  "multipart": [
    {
      "apply": {
        "model": "space:block/pipe"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "space:block/pipe_connector"
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "y": 180
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "y": 90
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "y": 270
      }
    },
    {
      "when": {
        "up": "true"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "x": 270
      }
    },
    {
      "when": {
        "down": "true"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "x": 90
      }
    }
  ]
}