{
  "multipart": [
    {
      "apply": {
        "model": "space:block/pipe"
      }
    },
    {
      "when": {
        "north": "other"
      },
      "apply": {
        "model": "space:block/pipe_connector"
      }
    },
    {
      "when": {
        "south": "other"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "y": 180
      }
    },
    {
      "when": {
        "east": "other"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "y": 90
      }
    },
    {
      "when": {
        "west": "other"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "y": 270
      }
    },
    {
      "when": {
        "up": "other"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "x": 270
      }
    },
    {
      "when": {
        "down": "other"
      },
      "apply": {
        "model": "space:block/pipe_connector",
        "x": 90
      }
    },
    {
      "when": {
        "north": "self"
      },
      "apply": {
        "model": "space:block/pipe_connector_self"
      }
    },
    {
      "when": {
        "south": "self"
      },
      "apply": {
        "model": "space:block/pipe_connector_self",
        "y": 180
      }
    },
    {
      "when": {
        "east": "self"
      },
      "apply": {
        "model": "space:block/pipe_connector_self",
        "y": 90
      }
    },
    {
      "when": {
        "west": "self"
      },
      "apply": {
        "model": "space:block/pipe_connector_self",
        "y": 270
      }
    },
    {
      "when": {
        "up": "self"
      },
      "apply": {
        "model": "space:block/pipe_connector_self",
        "x": 270
      }
    },
    {
      "when": {
        "down": "self"
      },
      "apply": {
        "model": "space:block/pipe_connector_self",
        "x": 90
      }
    }
  ]
}